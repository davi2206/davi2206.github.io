---
layout: _base.njk
---

<!-- Main -->
<div id="main">
    <!-- Post -->
    <section class="post">
        <header class="major">
            <span class="date">{{ date }}</span>
            <h1>{{ title | safe }}</h1>
            <p>{{ summary | safe }}</p>
        </header>
        <div class="image main"><img src="{{ featuredImage }}" alt="" /></div>
        <p>{{ content | safe }}</p>
    </section>
</div>